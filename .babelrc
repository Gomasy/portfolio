{
    "presets": [
        [ "@babel/env", {
            "targets": {
                "browsers": [
                    "last 2 versions",
                    "not dead",
                    "not ie 11",
                    "not ie_mob 11",
                    "not android <= 4.4.4"
                ]
            },
            "useBuiltIns": "usage",
            "corejs": 3,
            "modules": false
        }]
    ],
    "plugins": [
        [ "@babel/plugin-transform-react-jsx", {
            "pragma": "h",
            "pragmaFrag": "''"
        }]
    ]
}
